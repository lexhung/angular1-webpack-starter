{
  "name": "angular1-webpack-starter",
  "version": "0.5.0",
  "description": "Component based Angular web development with Webpack.",
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": "PinkyJie/angular1-webpack-starter",
  "license": "MIT",
  "main": "src/app/index.js",
  "scripts": {
    "start": "webpack-dev-server --config=./cfg/webpack-dev.config.js",
    "build": "rm -rf ./_dist && webpack --config=./cfg/webpack.config.js --progress --mock --prod",
    "test": "rm -rf ./src/test/unit/results && karma start ./cfg/karma.config.js",
    "codecov": "cat ./src/test/unit/results/coverage/lcov.info | codecov",
    "test:watch": "karma start ./cfg/karma.config.js --watch",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "e2e": "protractor ./cfg/protractor.config.js",
    "server": "nohup http-server ./_dist &"
  },
  "author": {
    "name": "Hung Le",
    "email": "lexhung@gmail.com",
    "url": "https://github.com/lexhung"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "es6",
    "components",
    "angular",
    "webpack",
    "jade",
    "stylus",
    "ui-router",
    "material design"
  ],
  "devDependencies": {
    "angular-mocks": "~1.5.8",
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "codecov.io": "^0.1.6",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.9.1",
    "eslint-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.9.0",
    "http-server": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jasmine-core": "^2.3.4",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-junit-reporter": "^1.1.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "*",
    "karma-webpack": "^1.7.0",
    "null-loader": "^0.1.1",
    "oclazyload": "^1.0.8",
    "phantomjs-prebuilt": "*",
    "postcss-loader": "^1.1.0",
    "protractor": "^4.0.10",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2",
    "raw-loader": "^0.5.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.6",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.10.1",
    "yargs": "^6.3.0"
  },
  "dependencies": {
    "angular": "~1.5.8",
    "angular-animate": "~1.5.8",
    "angular-loading-bar": "^0.9.0",
    "angular-messages": "~1.5.8",
    "angular-ui-router": "^0.3.2",
    "animate.css": "^3.4.0",
    "hammerjs": "^2.0.4",
    "jquery": "~3.1.1",
    "materialize-css": "0.97.8"
  }
}
